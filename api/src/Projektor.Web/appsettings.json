{
  "AllowedHosts": "*",
  "Api": {
    "Contact": {
      "Email": "francispion@hotmail.com",
      "Name": "Francis Pion",
      "Url": "https://www.francispion.ca"
    },
    "Description": "Project management Web application.",
    "Environments": [ "Development" ],
    "JwtBearer": {
      "Description": "Enter your token in the input below.",
      "Enabled": true
    },
    "License": {
      "Name": "Use under MIT",
      "Url": "https://github.com/Utar94/Projektor/blob/develop/LICENSE"
    },
    "Title": "Projektor API",
    "Version": "1.0.0"
  },
  "Cors": {
    "AllowedHeaders": [ "Authorization", "Content-Type" ],
    "AllowedMethods": [ "GET", "POST", "PUT" ]
  },
  "Identity": {
    "SignIn": {
      "RequireConfirmedEmail": true
    },
    "User": {
      "RequireUniqueEmail": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
